{
  "name": "@bugence/core",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm exec eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.ts\" --no-error-on-unmatched-pattern --max-warnings 0",
    "lint:fix": "pnpm exec eslint \"src/**/*.{ts,tsx}\" \"tests/**/*.ts\" --no-error-on-unmatched-pattern --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate": "pnpm run build && pnpm run openapi",
    "openapi": "pnpm exec dotnet-nswag run ../../BugenceEditConsole/OpenApi/content.nswag /runtime:Net60"
  },
  "devDependencies": {}
}
