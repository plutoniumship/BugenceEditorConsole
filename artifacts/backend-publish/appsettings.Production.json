{
  "DomainRouting": {
    "PrimaryZone": "bugence.app",
    "WildcardRecordTarget": "",
    "EdgeIpAddress": "5.189.164.172",
    "PublishRoot": "Published",
    "VerificationInterval": "00:05:00",
    "AutoManageIisBindings": true,
    "PerProjectIisSites": true,
    "IisSiteNamePattern": "Bugence_{ProjectId}",
    "IisAppPoolPattern": "BugencePool_{ProjectId}",
    "CertificateStoreName": "My",
    "CertificateStoreLocation": "LocalMachine"
  },
  "Certificates": {
    "Provider": "Webhook",
    "Webhook": {
      "Endpoint": "",
      "ApiKey": "",
      "IgnoreSslErrors": false,
      "ExpectPfx": true,
      "StorePath": "App_Data/certificates"
    },
    "LocalAcme": {
      "Enabled": true,
      "AllowSelfSignedFallback": true,
      "IssueCommand": "& 'C:\\Tools\\win-acme\\issue-cert.ps1' -Domain '{domain}' -Apex '{apex}' -WebRoot '{webroot}' -Email 'admin@bugence.com'",
      "TimeoutSeconds": 600,
      "PreferredChallenge": "http-01",
      "CertificateStoreName": "My",
      "CertificateStoreLocation": "LocalMachine",
      "StorePath": "App_Data/certificates"
    }
  },
  "FeatureFlags": {
    "DynamicVeV1": true,
    "DynamicVeConfidenceV1": true,
    "DynamicVeInspectorProV1": true,
    "DynamicVeBindingDebugV1": true
  }
}
